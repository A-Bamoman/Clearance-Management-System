@using ClearanceManagementSystem.Controllers
@model List<UserShowController.UserShowData>

@using ClearanceManagementSystem.Models
@{
    ViewBag.Title = "Show";
    Layout = "~/Views/_LayoutPage1.cshtml";
    var _cms = new CMS();
}

<h2 style="text-align:center;padding-top:40px;padding-bottom:30px">سجل الطلبات</h2>


<table class="table">
    <tr>
        <th>
            <h3>
                أسم الطالب
            </h3>

        </th>
        <th>
            <h3>
                رقم قيد الطالب
            </h3>
        </th>
        <th>
            <h3>
                الكلية
            </h3>
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.STUNA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STUNU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COLLEGE)
            </td>
            <td>
                @Html.ActionLink("معاملة الطلب", "Editt", new { id = item.idu })
            </td>
        </tr>
    }

</table>
